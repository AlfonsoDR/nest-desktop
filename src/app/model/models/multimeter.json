{
  "id": "multimeter",
  "element_type": "recorder",
  "label": "multimeter",
  "params": [{
      "id": "interval",
      "label": "time interval of recording",
      "level": 3,
      "unit": "ms",
      "value": 1,
      "input": "tickSlider",
      "ticks": [
        0.001,
        0.01,
        0.1,
        1,
        10
      ]
    },
    {
      "id": "start",
      "label": "start time",
      "level": 2,
      "unit": "ms",
      "value": 0,
      "input": "valueSlider",
      "max": 1000,
      "min": 0,
      "step": 1
    },
    {
      "id": "stop",
      "label": "stop time",
      "level": 2,
      "unit": "ms",
      "value": 1000,
      "input": "valueSlider",
      "max": 1000,
      "min": 0,
      "step": 1
    }
  ]
}
