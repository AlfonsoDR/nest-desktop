{
  "name": "nest-desktop",
  "version": "0.14.0",
  "description": "A NEST desktop application",
  "main": "main.js",
  "scripts": {
    "prestart": "node init.js",
    "start": "electron .",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "build": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=./src/assets/img/nest-icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babsey/nest-desktop.git"
  },
  "keywords": [
    "interactive",
    "neuronal",
    "network",
    "spike",
    "activity",
    "NEST"
  ],
  "author": {
    "name": "Sebastian Spreizer"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-notify": "^3.1.3",
    "bootstrap-slider": "^10.0.0",
    "clipboard": "^1.7.1",
    "colorbrewer": "^1.0.0",
    "d3": "^4.11.0",
    "flat": "^4.0.0",
    "font-awesome": "^4.7.0",
    "joi": "^13.1.1",
    "jquery": "^3.2.1",
    "jsonfile": "^4.0.0",
    "mathjs": "^3.16.4",
    "nedb": "^1.8.0",
    "numeric": "^1.2.6",
    "object-hash": "^1.1.8",
    "pouchdb": "^6.3.4",
    "pouchdb-upsert": "^2.2.0",
    "request": "^2.83.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "debug": "^3.1.0",
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "electron-rebuild": "^1.6.0"
  }
}
